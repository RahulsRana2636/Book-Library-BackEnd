{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": { "maxDuration": 30 }
        }
    ],
    "routes":[
        { "src": "/user/createuser", "dest": "index.js" },
        { "src": "/(.*)", "dest": "index.js" }
      ]
    
}